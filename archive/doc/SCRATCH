-- | A partially-processed reason section
type ReasonSection (direction âˆ· Direction) (defeasibility âˆ· Defeasibility) =
    ( ProblemReasonName
    , Text â Æ®Reason direction defeasibility
    , Text â Æ®ProblemVariables
    , ProblemStrengthDegree
    )

_rsProblemReasonName âˆ· ReasonSection direction defeasibility â†’ ProblemReasonName
_rsProblemReasonName (n, _, _, _) = n

_rsProblemReasonText âˆ· ReasonSection direction defeasibility â†’ Text â Æ®Reason direction defeasibility
_rsProblemReasonText (_, t, _, _) = t

_rsProblemVariables âˆ· ReasonSection direction defeasibility â†’ Text â Æ®ProblemVariables
_rsProblemVariables (_, _, v, _) = v

_rsProblemStrengthDegree âˆ· ReasonSection direction defeasibility â†’ ProblemStrengthDegree
_rsProblemStrengthDegree (_, _, _, d) = d
















